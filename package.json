{
  "name": "@wtyeung/n8n-nodes-puppeteer",
  "version": "1.5.1",
  "description": "n8n node for browser automation using Puppeteer (fork with TypeScript improvements and modern dev tooling)",
  "license": "MIT",
  "homepage": "https://github.com/wtyeung/n8n-nodes-puppeteer",
  "author": {
    "name": "laHeud",
    "email": "clement@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wtyeung/n8n-nodes-puppeteer.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "format": "prettier --write \"nodes/**/*.ts\"",
    "format:check": "prettier --check \"nodes/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "n8n",
    "node",
    "puppeteer",
    "scraper",
    "screenshot",
    "script",
    "pdf",
    "n8n-node",
    "n8n-community-node-package"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Puppeteer/Puppeteer.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "gulp": "^5.0.0",
    "n8n-workflow": "*",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@n8n/vm2": "^3.9.25",
    "puppeteer": "^24.0.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-human-typing": "^1.0.2",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
